{
  "name": "react-init",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@sentry/browser": "^5.5.0",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "apollo-link-state": "^0.4.2",
    "emotion-theming": "^10.0.14",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "i18next": "^17.0.7",
    "jest-dom": "^4.0.0",
    "jest-emotion": "^10.0.14",
    "logrocket": "^1.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.11.4",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "source-map-explorer": "^2.0.1",
    "why-did-you-update": "^1.0.6"
  },
  "scripts": {
    "dev": "PORT=8080 react-scripts start",
    "start": "yarn build && serve -s build -p 8080",
    "build": "react-scripts build",
    "test": "is-ci \"test:cov\" \"test:watch\"",
    "test:watch": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "test:cov": "react-scripts test --coverage",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test start http://localhost:8080 cy:run",
    "test:e2e:dev": "start-server-and-test start http://localhost:8080 cy:open",
    "validate": "yarn test:cov && yarn test:e2e:run",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@storybook/addon-a11y": "5.1.9",
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-events": "5.1.9",
    "@storybook/addon-links": "5.1.9",
    "@storybook/addons": "5.1.9",
    "@storybook/react": "5.1.9",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.7",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "cypress": "^3.4.1",
    "cypress-testing-library": "^4.0.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-cypress": "^2.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "is-ci-cli": "^1.1.1",
    "serve": "^11.1.0",
    "start-server-and-test": "^1.9.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/utils/react-testing-library-custom.js",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
}
