{
  "name": "react-init",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "@rematch/core": "^1.0.6",
    "@sentry/browser": "^4.5.3",
    "emotion-theming": "^10.0.6",
    "i18next": "^14.0.1",
    "jest-dom": "^3.0.0",
    "jest-emotion": "^10.0.6",
    "logrocket": "^0.6.19",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-i18next": "^9.0.8",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-testing-library": "^5.4.4",
    "source-map-explorer": "^1.6.0",
    "why-did-you-update": "^1.0.6"
  },
  "scripts": {
    "dev": "PORT=8080 react-scripts start",
    "start": "yarn build && serve -s build -p 8080",
    "build": "react-scripts build",
    "test": "is-ci \"test:cov\" \"test:watch\"",
    "test:watch": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "test:cov": "react-scripts test --coverage",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test start http://localhost:8080 cy:run",
    "test:e2e:dev": "start-server-and-test start http://localhost:8080 cy:open",
    "validate": "yarn test:cov && yarn test:e2e:run",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-a11y": "^v5.0.0-alpha.8",
    "@storybook/addon-actions": "^v5.0.0-alpha.8",
    "@storybook/addon-events": "^v5.0.0-alpha.8",
    "@storybook/addon-links": "^v5.0.0-alpha.8",
    "@storybook/addons": "^v5.0.0-alpha.8",
    "@storybook/react": "^v5.0.0-alpha.8",
    "babel-loader": "8.0.4",
    "cypress": "^3.1.4",
    "cypress-testing-library": "^2.3.5",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "is-ci-cli": "^1.1.1",
    "serve": "^10.1.1",
    "start-server-and-test": "^1.7.11"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/utils/react-testing-library-custom.js",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
}
